{
  "name": "jsfarm",
  "version": "0.1.0",
  "description": "Safe sandboxed JS code execution",
  "scripts": {
    "test": "jest",
    "dev": "ts-node-dev --respawn --transpileOnly ./src/index.ts",
    "build": "tsc",
    "serve": "node ./build/index.js",
    "lint": "tsc --noEmit && prettier --check \"src/*.{js,ts,tsx}\" \"src/**/*.{js,ts,tsx}\" && eslint --ext js,ts,tsx src",
    "lintfix": "prettier --write \"src/*.{js,ts,tsx}\" \"src/**/*.{js,ts,tsx}\" && eslint --ext js,ts,tsx --fix src"
  },
  "author": "Harry Brundage",
  "license": "MIT",
  "dependencies": {
    "@types/express-winston": "^3.0.1",
    "@zeit/ncc": "^0.15.1",
    "body-parser": "^1.18.3",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-winston": "^3.1.0",
    "generic-pool": "^3.7.1",
    "lodash": "^4.17.11",
    "lru-cache": "^5.1.1",
    "moment": "^2.24.0",
    "split": "^1.0.1",
    "typescript": "^3.3.3",
    "vm2": "^3.6.10",
    "winston": "^3.2.1",
    "xstate": "^4.3.2",
    "xxhash": "^0.2.4"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.1",
    "@types/generic-pool": "^3.1.9",
    "@types/jest": "^24.0.11",
    "@types/lru-cache": "^5.1.0",
    "@types/node": "^11.10.4",
    "@types/split": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^1.3.0",
    "@typescript-eslint/parser": "^1.3.0",
    "eslint": "^5.15.3",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-jest": "^22.4.1",
    "jest": "^24.5.0",
    "prettier": "^1.16.4",
    "ts-jest": "^24.0.0",
    "ts-node-dev": "^1.0.0-pre.32"
  }
}
